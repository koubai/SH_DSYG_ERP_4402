			res05,res06,res07,res08,res09,res10,keyword,createuid,createdate,updateuid,updatedate)
		values(#belongto#,#fieldno#,#brand#,#item1#,#item2#,#item3#,#tradename#,#typeno#,#color#,#packaging#,
